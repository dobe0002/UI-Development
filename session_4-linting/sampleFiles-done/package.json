  "scripts": {
    "build": "echo 'build not defined yet'",
    "clean": "rm -f package-lock.json && rm -rf node_modules && rm -rf build && npm i && npm run build",
    "eslint": "esw ",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:watch": "esw --watch ",
    "prettier:check": "prettier  --ignore-path ./.eslintignore --check '**/*.+(js|jsx|html|css|scss|json)'",
    "prettier:write": "prettier  --ignore-path ./.eslintignore --write '**/*.+(js|jsx|html|css|scss|json)'",
    "prettier:watch": "onchange '**/*.+(js|jsx|html|css|scss|json)' -- prettier --write {{changed}}",
    "prettier:writeAll": "npm run prettier:write && npm run eslint:fix"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-watch": "^6.0.1",
    "onchange": "^6.1.0",
    "prettier": "1.19.1",
